{
    "name": "amiralishabani/laravel-aash-captcha",
    "description": "Laravel wrapper for aash-captcha.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amirali Shabani",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "AashCaptcha\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AashCaptcha\AashCaptchaServiceProvider"
            ],
            "aliases": {
                "AashCaptcha": "AashCaptcha\Facades\AashCaptcha"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}