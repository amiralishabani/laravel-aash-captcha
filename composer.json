{
  "name": "amiralishabani/laravel-aash-captcha",
  "description": "A Laravel package for generating customizable image and audio CAPTCHAs.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amirali Shabani",
      "email": "youremail@example.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "AashCaptcha\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AashCaptcha\\AashCaptchaServiceProvider"
      ],
      "aliases": {
        "AashCaptcha": "AashCaptcha\\Facades\\AashCaptcha"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}